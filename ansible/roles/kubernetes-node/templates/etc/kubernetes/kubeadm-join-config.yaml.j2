---
apiVersion: kubeadm.k8s.io/v1beta1
kind: JoinConfiguration
discovery:
  bootstrapToken:
    token: {{ bootstrap_token }}
    apiServerEndpoint: {{ cluster_fqdn }}:{{ api_lb_port }}
    caCertHashes:
      - {{ ca_cert_hash }}
nodeRegistration:
  kubeletExtraArgs:
    node-labels: node-role.kubernetes.io/worker=
    event-qps: "0"
